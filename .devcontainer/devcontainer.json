	"image": "ubuntu:22.04",

	// Setup the workspace and mount on docker:
	"workspaceFolder": "/home/workspace",
	"mounts": [
		"source=${localWorkspaceFolder}/include,target=/home/workspace/include,type=bind",
		"source=${localWorkspaceFolder}/lib,target=/home/workspace/lib,type=bind",
		"source=${localWorkspaceFolder}/src,target=/home/workspace/src,type=bind",
		"source=${localWorkspaceFolder}/CMakeLists.txt,target=/home/workspace/CMakeLists.txt,type=bind"
	]
